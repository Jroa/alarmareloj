<html>
<head>
    <link rel="stylesheet" href="css/style.css">
    <script
    src="https://code.jquery.com/jquery-3.2.1.min.js"
    integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
    crossorigin="anonymous"></script>
    <script src="js/custom.js"></script>
</head>
<body>
  <div id="select_time">
    <div id="current_time">Hora Actual es: <span>02:44:02</span></div>
      
      <div id="hours_one" class="digit" style="left: 42px;">0</div>
      <div id="hours_two" class="digit" style="left: 86px;">0</div>
      <div id="divider">:</div>
      <div id="minutes_one" class="digit" style="left: 150px;">0</div>
      <div id="minutes_two" class="digit" style="left: 196px;">0</div>
      
      <div id="button_theme_day" class="theme_button none" style="display: none;">Day Mode</div>
      <div id="button_plus_hour" class="clock_button"></div>
      <div id="button_minus_hour" class="clock_button"></div>
      <div id="button_plus_min" class="clock_button"></div>
      <div id="button_minus_min" class="clock_button"></div>
      <div id="select_time_overlay"></div>
  </div>
  <div id="set_alarm_button" class="button" style="">Definir Alarma</div>
  <div id="alarm_set" style="display: none;">
    <div id="alarm_set_text" style="color: rgb(51, 51, 51);">Alarmar definida!</div>
    <div id="cancel_button" class="button">Cancelar</div>
  </div>
  <div id="alarm_start" class="none">
      <div id="wakeup">Sonando</div>
      <div id="stopor">
        <div id="stop_alarm_button" class="button">Parar</div>
      </div>
    </div>
</body>
</html>